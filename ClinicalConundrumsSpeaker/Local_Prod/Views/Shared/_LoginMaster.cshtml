        <div class="container overlay-z2 footer">
            <div class="row">
                <div class="col-sm-12 col-sm-12 col-md-2">
                    <div class="logo-img-ftr">
                        <object class="logo-img-ftr-svg" type="image/svg+xml" data="/Images/CHRClogos_mer.svg"></object>
                    </div>
                </div>

                <div class="hidden-xs  col-sm-12 col-md-4" style="margin-top:20px">This program has received an educational grant and in-kind support from BMS/Pfizer Alliance Canada</div>
                <div class="col-xs-12  col-sm-12 col-md-2">
                    <span style="margin-right: 20px">http://clinicalconundrums.ca</span>
                    <br class="r-break">
                    <a href="mailto:info@clinicalconundrums.ca" title="info@clinicalconundrums.ca" style="margin-right: 20px">info@clinicalconundrums.ca</a><br class="">
                    259 Yorkland Road, Suite 200, <br class="r-break">
                    North York, Ontario, <br class="r-break">
                    Canada, M2J 0B5
                    <br class="r-break">
                    <br>
                </div>
                <div class="col-xs-12  col-sm-12 col-md-2">
                    Dija Changkit<br>
                    Project Leader<br>
                    <a href="mailto:changkitdija@chrc.net" title="changkitdija@chrc.net">changkitdija@chrc.net</a><br>
                    416-977-8010 ext. 296
                </div>


                <div class="col-xs-12  col-sm-12 col-md-2">
                    Annie Pham <br>
                    Administrative Assistant<br>
                    <a href="mailto:phama@chrc.net" title="phama@chrc.net ">phama@chrc.net </a><br>
                    416-977-8010 ext. 244
                </div>
            </div>
        </div>
    </footer>
    <!-- end of footer-->
    @RenderSection("Scripts", required: false)

</body>
</html>
