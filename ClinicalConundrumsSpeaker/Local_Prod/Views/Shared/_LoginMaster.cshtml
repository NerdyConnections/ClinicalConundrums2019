<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="https://fonts.googleapis.com/css?family=BenchNine" rel="stylesheet">

    <link rel="icon" href="data:;base64,iVBORw0KGgo=">
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
         


    <link href="/Content/bootstrap.min.css" rel="stylesheet" />

    <link href="/Content/Login.css" rel="stylesheet" />
</head>
<body style="background-image: url(/Images/notebook1st.jpg);">
    <div class="overlay"></div>

    <!-- nav bar-->
    <header>
        <div class="navbar navbar-inverse overlay-z2">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>


                            <div id="logo">
                                <div style="" class="logo-img">
                                    <object id="logo-svg" type="image/svg+xml" data="/Images/BMSPfizer.svg"></object>
                                </div>
                                <div class="logo-txt">
                                    <!--div>COMPANY LOGO</div-->
                                </div>
                                <div class="logo-txt-sub">Speaker Resource Portal</div>
                                <div class="logo-txt-welcome">&nbsp;</div>
                            </div>
                        </div>

                        <div class="navbar-collapse collapse">
                            <ul class="nav navbar-nav float-right">
                                <li><a href="#" title="Français FMOQ">Français FMOQ</a></li>
                                <!--li><a href="#">Help Centre</a></!--li>
                                <li><a href="#">Contact Us</a></li>
                                <li<a href="#">Log Out</a></li-->
                            </ul>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </header>
    <!--end of nav bar-->

    <section>

        <div role="main" class="container overlay-z2">

            @RenderBody()
        </div>
    </section>
    <!--footer-->
